<div>
    <form action="http://localhost:8080/api/carts/" method="POST">
        <button type="submit">Create new Cart</button>
    </form>
    <ul style="list-style-type: none">
        {{#each carts}}
        <li>
            <form action="http://localhost:8080/api/carts/{{this._id}}" method="GET">
                <button type="submit">{{this._id}}</button>
            </form>
        </li>
        <br>
        {{/each}}
    </ul>
</div>